{
  "name": "seeui-mobile-webpack-demo",
  "version": "1.0.0",
  "description": "A Simple SeeUI Mobile Demo",
  "main": "./src",
  "scripts": {
    "lint": "fecs src --type=js --reporter=baidu || true",
    "build": "cross-env NODE_ENV=production webpack --config ./tool/webpack.build.babel.js",
